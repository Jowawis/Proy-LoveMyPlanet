<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lovemyplanet">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">
        <activity
            android:name=".ui.main.CanjearActivity"
            android:exported="true"
            android:theme="@style/Theme.LoveMyPlanet"/>
        <activity
            android:name=".ui.main.ActividadActivity"
            android:exported="true"
            android:theme="@style/Theme.LoveMyPlanet" />
        <activity
            android:name=".ui.main.CrearActividadActivity"
            android:exported="true"
            android:theme="@style/Theme.LoveMyPlanet" />
        <activity
            android:name=".TappedActivity"
            android:exported="true"
            android:label="@string/title_activity_tapped"
            android:theme="@style/Theme.LoveMyPlanet.NoActionBar" />
        <activity
            android:name=".LoginActivity"
            android:exported="true"
            android:theme="@style/Theme.LoveMyPlanet">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>